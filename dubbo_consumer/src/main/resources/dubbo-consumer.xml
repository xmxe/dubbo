<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!--dubbo应用程序命名-->
    <dubbo:application name="dubbo-consumer"/>

    <!--dubbo注册地址-->
    <!--<dubbo:registry protocol="zookeeper" address="192.168.236.128:2181,192.168.236.129:2181,192.168.236.130:2181"/>-->
    <dubbo:registry address="zookeeper://192.168.236.128:2182"/>
    <!--接口引用-->
    <dubbo:reference check="false" interface="com.api.HelloDubboService" id="helloDubboService"/>
</beans>